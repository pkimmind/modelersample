{
  "initBlock": {
    "name": "SQLServerDriverNameInit",
    "type": "SESSION",
    "refreshPeriod": 86400,
    "connectionPool": "connectionPool:SQLDB_Northwind.Northwind Init Block Connections",
    "selectStatements": [
      {
        "databaseType": "DEFAULT",
        "query": "select case when 'valueof(NQ_SESSION.OS)' ='WINDOWS' then '{SQL Server Native Client 10.0}'
else 'SQLServer_Wire_Protocol' end from dual;"
      }
    ],
    "targetVariables": [
      {
        "name": "SQLSERVERDRIVER",
"Expr": {
  "expressionTemplate": "'{SQL Server Native Client 10.0}'",
}
      }
    ]
  }
}
